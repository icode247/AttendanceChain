mpt\n  if (!window.walletConnection.isSignedIn()) {\n    return (\n      <main className=\"signin\">\n        <h1>Welcome to AttendanceChain</h1>\n        <p style={{ textAlign: \"center\" }}>\n          Click the button below to sign in:\n        </p>\n        <p style={{ textAlign: \"center\", marginTop: \"2.5em\" }}>\n          <button onClick={login}>Sign in</button>\n        </p>\n      </main>\n    );\n  }\n  return (\n    // use React Fragment, <>, to avoid wrapping elements in unnecessary divs\n    <>\n      <header>\n        <div className=\"logo\"></div>\n        <button className=\"link\" style={{ float: \"right\" }} onClick={logout}>\n          Sign out <span className=\"id\">{window.accountId}</span>\n        </button>\n      </header>\n      <main>\n        <ListLessons lessons={lessons} />\n        <ListStudents />\n        <ListParents />\n      </main>\n    </>\n  );\n}\n"},"lineCount":null}},"error":null,"hash":"3509c1b8e6677e85a51831c6bf59004d","cacheData":{"env":{"NODE_ENV":"development"}}}